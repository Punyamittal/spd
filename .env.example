# SustainLABs Environment Variables Example
# Copy this file to .env and replace the values with your actual API keys

# Google Maps API Key - required for the heat map visualization
# Get your API key from https://developers.google.com/maps/documentation/javascript/get-api-key
VITE_GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY

# API Keys
VITE_GEMINI_API_KEY=your_gemini_api_key
VITE_WEATHER_API_KEY=your_weather_api_key

# Optional settings
VITE_DEFAULT_TONE=friendly
VITE_DEFAULT_LANGUAGE=en
VITE_ENABLE_VOICE=true
VITE_ENABLE_WEATHER=true

# Other environment variables may be added here in the future 